  object ProdQRY: TIBQuery
    Database = dm.fbdb
    Transaction = dm.IBTransaction1
    BufferChunks = 1000
    CachedUpdates = False
    GenerateParamNames = False
    GeneratorField.ApplyOnEvent = gaeOnNewRecord
    SQL.Strings = (
      'select PRODUCTO.BC AS BC,PRODUCTO.CODIGO AS CODIGO,'
      'PRODUCTO.DESCRIPCION AS DESCRIPCION,PRODUCTO.PRECIO AS PRECIO,'
      'PRODUCTO.PRECIODISTRIBUIDOR AS PRECIODISTRIBUDOR,'
      'PRODUCTO.PRECIOESPECIAL AS PRECIOESPECIAL,'
      'PRODUCTO.UNIDADMEDIDA AS UNIDADMEDIDA, PRODUCTO.EXISTENCIA AS '
      'EXISTENCIA'
      ' from PRODUCTO WHERE PLUEMPRESAS=:pluempresa and '
      ' UPPER(codigo) like ''A%'' AND'
      ' ACTIVO=''T'' ORDER BY descripcion asc'
    )
    Params = <    
      item
        DataType = ftUnknown
        Name = 'pluempresa'
        ParamType = ptInput
      end>
    DataSetCloseAction = dcDiscardChanges
    left = 144
