  object FACTMSQL: TZUpdateSQL
    DeleteSQL.Strings = (
      'DELETE FROM docclientesm'
      'WHERE'
      '  docclientesm.PLUDOCCLIENTES = :OLD_PLUDOCCLIENTES'
    )
    InsertSQL.Strings = (
      'INSERT INTO docclientesm'
      '  (PLUDOCCLIENTES, CODIGO, CONDICIONES, DESCUENTO, FECHA, NOMBRE, OBSERVACIONES, '
      '   ESEXENTO, GRAVADO, IVA, POST, TIPO, TOTAL, PLUCLIENTE, PLUEMPLEADOS, '
      '   PLUEMPRESAS, PLUSUCURSALES, FLETE, OCOMPRA, TIEMPOCREDITO, PAGADO, NOCHEQUE, '
      '   PLUBANCO, NOMBRETARJETA, NOTARJETA, TELTARJETA, FECHATARJETA, PLUTARJETACRED, '
