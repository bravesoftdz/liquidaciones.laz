    SQL.Strings = (
      'select'
      'A.PLUDOCCLIENTES,'
      'cast(a.fecha as date) AS FECHA,'
      'A.PLUEMPLEADOS,'
      'a.codigo,a.correlativo,a.tipo,a.plucliente,a.nombre,'
      'a.esexento,a.gravado,a.iva,a.post,a.total,b.nombre as vendedor,'
      'a.pluempresas,a.plusucursales,a.flete,a.pagado,a.diascredito,'
      'a.legal,a.formapago,a.plufacturanc,a.direccion_envio,a.pendiente,'
      'a.impreso,a.v_exento,a.anulado,a.pc_crea,a.id,a.dia,'
      'A.CONDICIONES,A.OBSERVACIONES'
      'from docclientesm a'
      'inner join empleados b on (a.pluempleados=b.pluempleados)'
      'where a.codigo=0'
